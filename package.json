{
  "name": "tmp-nextjs",
  "version": "2.0.4",
  "description": "Template for Next.js",
  "author": "@yogarasu",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && stylelint src/assets/css/**/*.css --fix --config stylelint.config.js",
    "sb": "npx sb dev -p 6006",
    "update": "pnpm -r update --latest --depth 100 && npx sb upgrade -s && pnpm install --lockfile-only && npm install --package-lock-only"
  },
  "dependencies": {
    "next": "^13.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.11",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-viewport": "^7.0.11",
    "@storybook/blocks": "^7.0.9",
    "@storybook/nextjs": "^7.0.7",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "^0.1.0",
    "@svgr/webpack": "^7.0.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "storybook": "^7.0.11",
    "stylelint": "^15.6.1",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "overrides": {
    "css-select": "^4.0.0",
    "got": "^11.8.5",
    "json5": "^2.2.2",
    "node-fetch": "^2.6.7",
    "cacheable-request": "^10.2.7"
  },
  "browserslist": [
    "> 1% in JP",
    "last 2 versions"
  ],
  "volta": {
    "node": "18.16.0",
    "npm": "9.5.1",
    "pnpm": "8.2.0"
  },
